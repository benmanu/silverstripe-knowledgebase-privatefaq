    "type": "silverstripe-module",
    "homepage": "http://github.com/benmanu/silverstripe-knowledgebase-privatefaq",
    "keywords": ["silverstripe", "faq", "search", "knowledgebase", "solr", "private", "faq"],
    "license": "BSD-3-Clause",
    "authors": [
      {"name": "Ben Manu","email": "ben@silverstripe.com"},
      {"name": "Tim Kung","email": "tkung@silverstripe.com"}
    ],
    "support": {
      "issues": "http://github.com/benmanu/silverstripe-knowledgebase-privatefaq/issues"
    },
    "require": {
